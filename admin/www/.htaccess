# Activation du module de réécriture d'URL :
RewriteEngine on
# RewriteBase "/.../"

#--------------------------------------------------
# ROUTES DES USERS
#--------------------------------------------------
# ROUTE DECO
# PATTERN: /users/logout
# TARGET: ?books=index
RewriteRule ^name\/logout$ index.php?name=logout [L]


# --------------------------------------------------
RewriteCond %{REQUEST_URI} !\.(js|css|jpg|gif|zip|png|pdf)$ [NC]